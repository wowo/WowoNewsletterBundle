{
    "name": "wowo/wowo-newsletter-bundle",
    "description": "Newsletter Bundle for Symfony2 (with messages queuing in beanstalkd)",
    "keywords": ["newsletter", "queue", "beanstalkd"],
    "type": "package",
    "license": "",
    "authors": [
        {
            "name":     "Wojciech Sznapka",
            "homepage": "http://sznapka.pl",
            "email":    "wojciech@sznapka.pl"
        }     
    ],
    "require": {        
        "wowo/wowo-queue-bundle": "1.0.2",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "symfony/swiftmailer-bridge": ">=2.1.0,<2.1.x-dev",        
        "symfony/console": ">=2.1.0,<2.1.x-dev",
        "symfony/framework-bundle": ">=2.1.0,<2.1.x-dev",
        "symfony/config": ">=2.1.0,<2.1.x-dev",
        "symfony/dependency-injection": ">=2.1.0,<2.1.x-dev",
        "symfony/http-kernel": ">=2.1.0,<2.1.x-dev",
        "symfony/validator": ">=2.1.0,<2.1.x-dev",
        "symfony/form": ">=2.1.0,<2.1.x-dev"
    },
    "require-dev": {
        "lapistano/proxy-object": "dev-master",
        "mockery/mockery" : "0.7.2"
    },
    "suggest": {
        "stfalcon/tinymce-bundle": "master"
    },
    "autoload": {
        "psr-0": { "Wowo\\NewsletterBundle": "" }
    },
    "target-dir": "Wowo/NewsletterBundle",
    "minimum-stability": "dev"
}
